import java.math.BigInteger;
import java.util.Random;


public class RandomGen {

	public int genRand(int size){
		byte[] result = new byte[size];
		BigInteger b;
		
		Random r = new Random();
		r.nextBytes(result);
		
		b=new BigInteger(result);
		System.out.println(b);
		
		return result.;
	}
	
}
