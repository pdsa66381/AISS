import java.util.Random;

public class RandomGen {
	
	public RSABigInteger genRandInput(int size){
		
		unsigned byte[] buffer = new byte[size];
		RSABigInteger result;
		Random r = new Random();
		
		r.nextBytes(buffer);
		buffer[0]=0;
		result=new RSABigInteger(buffer);
		
		return result;
	}
	
}
